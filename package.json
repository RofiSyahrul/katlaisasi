{
  "name": "katlaisasi",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "fetch-words": "node -r esbuild-register scripts/fetch-words.ts",
    "lint": "prettier --check . && eslint .",
    "partytown": "partytown copylib static/~partytown",
    "pre-commit": "lint-staged && yarn test --changed origin/main",
    "predev": "yarn partytown",
    "prebuild": "yarn partytown",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:playwright": "playwright test",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@playwright/test": "^1.25.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitest/coverage-c8": "^0.23.2",
    "autoprefixer": "^10.4.11",
    "cosmiconfig-typescript-loader": "^4.0.0",
    "esbuild": "^0.15.7",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.46.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  },
  "type": "module",
  "dependencies": {
    "@builder.io/partytown": "^0.7.0",
    "@liveblocks/client": "^0.17.11",
    "@liveblocks/node": "^0.17.11",
    "@supabase/supabase-js": "^1.35.6",
    "fast-deep-equal": "^3.1.3"
  },
  "lint-staged": {
    "*.{svelte,ts,js}": [
      "prettier --check",
      "eslint"
    ]
  }
}
